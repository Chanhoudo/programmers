      "source": [
        "def solution(storage, usage, change):\n",
        "    total_usage = 0\n",
        "    for i in range(len(change)):\n",
        "        usage = usage + (usage * change[i]/100)\n",
        "        total_usage += usage\n",
        "        if total_usage > storage:\n",
        "            return i\n",
        "\n",
        "    return -1"
      ]
    }
  ]
